{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "plugins": ["prettier-plugin-tailwindcss"]
}
